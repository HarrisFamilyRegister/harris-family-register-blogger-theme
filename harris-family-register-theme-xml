<script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
  var backToTopButton = document.getElementById('back-to-top-fixed');
  var backToTopFooterButton = document.getElementById('back-to-top');
  
  function scrollToTop(e) {
    e.preventDefault();
    window.scrollTo({top: 0, behavior: 'smooth'});
  }

  backToTopButton.addEventListener('click', scrollToTop);
  backToTopFooterButton.addEventListener('click', scrollToTop);

  window.addEventListener('scroll', function() {
    if (window.pageYOffset > 100) {
      backToTopButton.style.display = 'block';
    } else {
      backToTopButton.style.display = 'none';
    }
  });
});
//]]>
</script>
 <script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
  var cyclePageLink = document.getElementById('random-page-link');
  var navButtons = document.querySelectorAll('.nav-button');
  var currentIndex = 0;

  cyclePageLink.addEventListener('click', function(e) {
    e.preventDefault();
    
    // Get the next page in the cycle
    var nextPage = navButtons[currentIndex].href;
    
    // Update the index for the next click
    currentIndex = (currentIndex + 1) % navButtons.length;
    
    // Navigate to the next page
    window.location.href = nextPage;
  });

  // Set the initial index based on the current page
  var currentPath = window.location.pathname;
  for (var i = 0; i < navButtons.length; i++) {
    if (navButtons[i].pathname === currentPath) {
      currentIndex = (i + 1) % navButtons.length;
      break;
    }
  }
});
//]]>
</script>
 <script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
    // Select all grid items that are supposed to be clickable
    const gridItems = document.querySelectorAll('.grid-item');

    // Add a click event listener to each grid item
    gridItems.forEach(item => {
        item.addEventListener('click', function() {
            const link = item.querySelector('a.full-link');
            if (link && link.href) {
                window.open(link.href, '_blank');
            }
        });
    });
});
//]]>
</script>
  </body>
</html>
